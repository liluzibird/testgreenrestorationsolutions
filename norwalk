<script>
    window.customWidgetsFunctions["42042a6c1b6743f58823dcac2be37a91~8"] = function (element, data, api) {
        // The counter is made as a class so that no problems arise when having multiple instances of this widget on a page.
// This looks for any class that hasnt been randomized. It then randomizes the class and changes the class accordingly.
var notRand = document.getElementsByClassName("rand_notRand");
// Grab the total notRand elements and use it to loop through each element replacing rand_notRand
var totalNotRand = notRand.length;
// This randomization allows multiple instances of the widget to be used.
var randomClass = Math.ceil(Math.random() * 10000000).toString().padStart(8, 0);

var rand = "rand_" + randomClass;

for(var i = 0; i < totalNotRand; i++)
{
    notRand[0].className = rand + notRand[0].className.substring(12, notRand[0].className.length);
}
    };
</script>
<script>
    window.customWidgetsFunctions["2b30481ee92a40d99b40fe576ac56f9d~25"] = function (element, data, api) {
        dmAPI.runOnReady("searchPageMain", () => {
    const popupContainer = element.querySelector('#pageResultsPopupContainer');
    const popupClose = element.querySelector("#resultsPopupClose");
    
    if (data.config.popupWindowEnabled) {
        popupContainer.addEventListener("click", (e) => {
            if (e.target.id === "pageResultsOverlay") {
                popupContainer.setAttribute("hidden", true);
            }
        });
        
        popupClose.addEventListener("click", () => {
            popupContainer.setAttribute("hidden", true);
        });
        
        detectSubmitButton(20);
    }
    
    // Keep trying to detect the submit button so that the popup functionality works.
    function detectSubmitButton(tries) {
        if (tries <= 0) {
            // Out of attempts
            return false;
        }
        if (document.getElementById("searchSubmit")) {
            document.getElementById("searchSubmit").addEventListener("click", (e) => {
                popupContainer.setAttribute("hidden", false); // Display the popup
            });
            
            // Successfully found the submit button
            return true;
        }
        else {
            // Recursively retry after a half second delay.
            return window.setTimeout(() => { 
                return detectSubmitButton(tries-1);
            }, 500);
        }
    }
});
    };
</script>
