		  'site_alias':  _dm_gaq.siteAlias,
		  'product': 'DM_DIRECT',
		  'plan': 'BUSINESS_PLUS',
		  'dfooter': 'false',
		  'sitetype': '1',
		  'pwaEnabled': 'true'
	  }

	  _dm_gaq.pushEvent = function(trackingId, event, category, label, value, additionalParams) {
	  	  var initialProps = {
			  send_to: trackingId,
			  event_category: category,
			  event_label: label,
			  value: value
		  }
		  additionalParams = additionalParams || {};
	  	  var eventProps = $.extend({}, initialProps, additionalParams, trackingId === _dm_gaq.systemAggregatedGaqID?_dm_gaq.gaAggregatedEventAttributes:{});
		  gtag('event', event, eventProps);
	  }

		window.dataLayer = window.dataLayer || [];
		function gtag(){dataLayer.push(arguments);}
		gtag('js', new Date());

	  function pushInsiteImpressions(id) {
		  $.each(_dm_insite, function(idx, rule) {
			  _dm_gaq.pushEvent(id, 'insite_impression', 'insite', rule.ruleType + '__' + rule.ruleId);
		  });
	  }

	if(_dm_gaq.systemAggregatedGaqID){
		// track aggregated account
		gtag('config', '', {
			'send_page_view': false,
			'anonymize_ip': true,
			'transport_type': 'beacon',
			'custom_map': {'dimension1': 'site_alias', 'dimension2': 'product', 'dimension3': 'dFooter', 'dimension4': 'sitetype'}
		});
		gtag('set', 'developer_id.dMDU5Nj', true);

		_dm_gaq.pushEvent('', 'page_view');
		pushInsiteImpressions(_dm_gaq.systemAggregatedGaqID);
	}

	if(_dm_gaq.externalGaqID && _dm_gaq.externalGaqID!= '') {
		gtag('config', 'UA-206360980-1', {
			'send_page_view': false,
			'anonymize_ip': true,
			'transport_type': 'beacon',
			'linker': {
				'domains': ['gogreenrestoration.com']
			}
		});
		_dm_gaq.pushEvent('UA-206360980-1', 'page_view');
		pushInsiteImpressions(_dm_gaq.externalGaqID);
	}

