<script>
    var isWLR = true;

    window.customWidgetsFunctions = {};
    window.customWidgetsStrings = {};
    window.collections = {};
    window.currentLanguage = "ENGLISH"
    window.isSitePreview = false;
</script>
<script>
    window.customWidgetsFunctions["87d8d84f0ab348b48c585ab52a86aced~1"] = function (element, data, api) {
        /* * Breadcrumbs
* August 23rd, 2017 
* Version 1.0.0
* Version 1.1.0
- Changed the color text to dropdown for each link texts
- Added options to make the current text link to be bold.
- Added design option to the current text link.

*Upon purchase of this widget, you acquire the right to use it; you're not actually 
acquiring the widget itself.TheCamel.co is the owner of the intellectual property of this widget.
The widget is for use only, it is not to be on sold, lent, rented, given away, gifted you can’t 
even put it in your will. It is for use for your sites only on the duda dashboard we install the item in.

*Kindly refer to our Terms and Conditions for more info - http://www.thecamel.co/terms-and-conditions
*/

let dividerStyle = data.config.dividerStyle;
let iconSize = data.config.iconSize;
let iconColor = data.config.iconColor;
let $element = $(element);
//change navItemSelected for desktop and mobile
let $navItemSelected = data.device == "mobile" ? $('#mobile-hamburger-drawer .dmNavItemSelected') : $('#hcontainer .dmNavItemSelected');
previousNavs = [];
navsArray = [];
let navs = '<li class="breadLink dividerStyle"><a class="breadLinktext" href="../index.html">HOME</a><i class="breadCicon fa fa-'+dividerStyle+'" style="color:'+iconColor+'; font-size:'+iconSize+'" aria-hidden="true"></i></li>';

if($navItemSelected.parent().parent().prev()){
    $element.find('.breadCrumbUL').html("");
    //add navselected data attrib to the current selected nav
    $navItemSelected.attr('data-nav-selected','true');
    //This code fixes the Bug when you refresh the page(BUG:Parent of the selected nav disappears), so we need to add it again.
	$navItemSelected.parent().parent().prev().attr('data-nav-selected','true');
    //same as above check again if this is the main menu of nav
	if($navItemSelected.parent().parent().prev().parent().parent().prev()){
		$navItemSelected.parent().parent().prev().parent().parent().prev().attr('data-nav-selected','true');
	}
	
	previousNavs = $('[data-nav-selected]');//previousNavs = $('#hcontainer [data-nav-selected]');
    
	//convert previousNavs to array to map through it.
	navsArray = previousNavs.toArray();
    
    navsArray.forEach(function(nav){
        navs += '<li class="breadLink dividerStyle"><a class="breadLinktext" href="https://www.gogreenrestoration.com/services/'&#32;+&#32;nav.href&#32;+&#32;'">' + nav.text +'</a> <i class="breadCicon fa fa-'+dividerStyle+'" style="color:'+iconColor+'; font-size:'+iconSize+'" aria-hidden="true"></i></li>';
    });
	
	$element.find('.breadCrumbUL').append(navs);
	$element.find('.breadCrumbUL li').last().find('a').addClass('activeBreadLink');
	$element.find('.breadCrumbUL li').last().find('i').css('display','none');
    previousNavs = [];
    navsArray = [];
}
    };
</script>
<script>
    window.customWidgetsFunctions["42042a6c1b6743f58823dcac2be37a91~8"] = function (element, data, api) {
        // The counter is made as a class so that no problems arise when having multiple instances of this widget on a page.
// This looks for any class that hasnt been randomized. It then randomizes the class and changes the class accordingly.
var notRand = document.getElementsByClassName("rand_notRand");
// Grab the total notRand elements and use it to loop through each element replacing rand_notRand
var totalNotRand = notRand.length;
// This randomization allows multiple instances of the widget to be used.
var randomClass = Math.ceil(Math.random() * 10000000).toString().padStart(8, 0);

var rand = "rand_" + randomClass;

for(var i = 0; i < totalNotRand; i++)
{
    notRand[0].className = rand + notRand[0].className.substring(12, notRand[0].className.length);
}
    };
</script>
<script type="text/javascript">

    var d_version = "23..34";
    var build = "2023-07-13T12_51_26";
    window['v' + 'ersion'] = d_version;

    function buildEditorParent() {
        window.isMultiScreen = true;
        window.editorParent = {};
        window.previewParent = {};
        window.assetsCacheQueryParam = "?version=2023-07-13T12_51_26";
        try {
            var _p = window.parent;
            if (_p && _p.document && _p.$ && _p.$.dmfw) {
                window.editorParent = _p;
            } else if (_p.isSitePreview) {
                window.previewParent = _p;
            }
        } catch (e) {

        }
    }

    buildEditorParent();


</script>
